<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#C58D5F" />
            <stroke
                android:width="1dp"
                android:color="#C58D5F" />
            <corners
                android:radius="1dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="#DB9D6A" />
            <stroke
                android:width="1dp"
                android:color="#C58D5F" />
        </shape>
    </item>
</selector>